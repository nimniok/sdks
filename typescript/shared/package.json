{
  "name": "@1inch/sdk-shared",
  "version": "1.0.0",
  "description": "Shared utilities and types for 1inch SDKs",
  "author": "@1inch",
  "homepage": "https://github.com/1inch/sdks#readme",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "build": "rm -rf dist && tsdown",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "type-check": "tsc --noEmit -p tsconfig.lib.json",
    "clean": "rimraf dist node_modules",
    "changelog:generate": "changelog generate -a"
  },
  "dependencies": {
    "viem": "^2.21.0",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@types/node": "^22.13.0",
    "tsdown": "^0.2.17",
    "typescript": "^5.7.3",
    "vitest": "^3.2.4"
  },
  "volta": {
    "node": "22.13.1"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
